pipeline {
    agent any
    stages {
        stage('working with condition') {
            steps {
                script{
                    int a=10
                    int b=100
                     if (a>b)
                    {
                        println "a value is big :${a}"
                    }
                    else {
                        println "b value is big: ${b}"
                    }
                }
            }
        }
    }
}
