pipeline {
    agent any
    stages {
        stage('file handling') {
            steps {
                script{
                    File f1 = new file("/tmp/test.txt")
                    def lines = f1.readLines()
                    println "${lines}"
                    for(line in lines) {
                        println "myline is ${line}"
                    }
                }
            }
        }
    }
}
